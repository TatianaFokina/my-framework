//-Repeat
- for (var i = 1; i <= limit; i++)
	block



//-Breadcrumbs
mixin breadcrumbs($items, $last)
	each $url, $item in $items
		li.breadcrumb-item
			//-if breadcrumb is single
			if (items.length === 1)
				span= $url
			else
				a(href=$url).breadcrumb-item__lnk= $item
	if last
		li.breadcrumb-item.active= $last
	else
